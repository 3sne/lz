{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/fonts/ibm-plex-sans-var.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/ibm-plex-sans-var-italic.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/LeeRobinson.pdf",
      "destination": "/robinson-resume-2019.pdf"
    },
    {
      "source": "/robinson-resume-2019.pdf",
      "destination": "https://read.cv/leerob"
    },
    {
      "source": "/resume",
      "destination": "https://read.cv/leerob"
    },
    {
      "source": "/projects",
      "destination": "/about"
    }
  ]
}
